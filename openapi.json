    }
  ],
  "paths": {
    "/": {
      "get": {
        "summary": "Root",
        "operationId": "root_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/privacy": {
      "get": {
        "summary": "Privacy Policy",
        "operationId": "privacy_policy",
